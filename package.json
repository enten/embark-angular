{
  "name": "embark-angular",
  "version": "0.1.0",
  "description": "Angular Decentralized Application with Embark",
  "license": "MIT",
  "author": "Steven Enten <steven@enten.fr>",
  "keywords": [
    "embark",
    "angular",
    "typescript",
    "dapp"
  ],
  "homepage": "https://github.com/enten/embark-angular",
  "bugs": {
    "url": "https://github.com/enten/embark-angular/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enten/embark-angular"
  },
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "embark test"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "coveragePathIgnorePatterns": [
      "^.+\\.d\\.ts$",
      "<rootDir>/.storybook/",
      "<rootDir>/src/jestGlobalMocks.ts",
      "<rootDir>/src/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "bootstrap": "^4.1.3",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.2",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@babel/core": "^7.1.2",
    "@storybook/addon-actions": "~4.0.0-rc.1",
    "@storybook/addon-knobs": "~4.0.0-rc.1",
    "@storybook/angular": "~4.0.0-rc.1",
    "@types/node": "~10.12.0",
    "@types/webpack-env": "~1.13.6",
    "babel-loader": "~8.0.4",
    "codelyzer": "~4.5.0",
    "jest": "~23.6.0",
    "jest-preset-angular": "~6.0.1",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  }
}
